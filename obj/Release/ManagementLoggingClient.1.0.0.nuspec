        <dependency id="AWSSDK.Core" version="4.0.0.22" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Logging" version="9.0.8" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.Logging.Abstractions" version="9.0.8" exclude="Build,Analyzers" />
      </group>
    </dependencies>
